
@using Test_GLP.Models

@model ViewUsers

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Master/main_layout.cshtml";
}

<meta name="viewport" content="width=device-width" />
<link href="@Url.Content("~/Content/GLPStyle.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/ControlStyle.css")" rel="stylesheet" type="text/css" />

<h2>Users</h2>
<div>
    <table>
        <tr>
            <th>User ID</th>
            <th>User Name</th>
            <th>User Password</th>
            <th>Created Date</th>
        </tr>
        @foreach (Users t in Model.LstUsers)
        {
            <tr>
                <th>@t.USER_ID</th>
                <th>@t.USER_NAME</th>
                <th>@t.USER_PASS</th>
                <th>@t.CREATED_DATE</th>
            </tr>
        }
    </table>
</div>
